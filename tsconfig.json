{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": false,
        "noLib": false,
        "declaration": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noEmitHelpers": true,
        "noEmitOnError": false,
        "lib": [
            "es6",
            "dom"
        ],
        "baseUrl": ".",
        "paths": {
            "*": [
                "./node_modules/tns-core-modules/*",
                "./node_modules/*"
            ]
        }
    },
    "files": [
        "node_modules/tns-core-modules/tns-core-modules.d.ts",
        "references.d.ts",
        "background-fetch.ios.ts",
        "background-fetch.android.ts"
    ],
    "exclude": [
        "node_modules",
        "platforms"
    ],
    "compileOnSave": false
}